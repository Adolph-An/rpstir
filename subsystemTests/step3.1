#!/bin/bash

# set environment variables if not set
THIS_SCRIPT_DIR=$(dirname $0)
. $THIS_SCRIPT_DIR/../envir.setup

. $RPKI_ROOT/trap_errors

# test functions
. $THIS_SCRIPT_DIR/test.include

echo "Loading data"
../rsync_aur/rsync_aur -t $RPKI_PORT -f test3.1a.log -w -e -i -d $RPKI_ROOT/testcases || \
  check_errs $? "$0 rsync_aur on test3.1a.log failed"
sleep 2
../rsync_aur/rsync_aur -t $RPKI_PORT -f test3.1b.log -w -e -i -d $RPKI_ROOT/testcases || \
  check_errs $? "$0 rsync_aur on test3.1b.log failed"
sleep 6

echo $0 PASS
