#!@SHELL_BASH@

# set environment variables if not set
THIS_SCRIPT_DIR=$(dirname $0)
. $THIS_SCRIPT_DIR/../../etc/envir.setup

. $RPKI_ROOT/lib/util/trap_errors

# test functions
. $RPKI_ROOT/tests/test.include

echo "Loading data and running garbage collector"
rsync_aur -s -t $RPKI_PORT -f test2.7.log -w -e -i -d $THIS_SCRIPT_DIR/testcases || \
  check_errs $? "$0 rsync_aur failed"
run "step7.garbage" garbage || \
  check_errs $? "$0 garbage failed"

echo $0 PASS
