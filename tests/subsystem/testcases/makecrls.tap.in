#!/bin/sh -e

@SETUP_ENVIRONMENT@

t4s_setup

mkcrl() {
    t4s_testcase "$1" './make_test_crl "$@"' "$@"
}

mkcrl L1 0D 1Y < "$TESTS_SRCDIR/makeL1"
mkcrl L11 0D 3M < "$TESTS_SRCDIR/makeL11"
mkcrl L111 -1M  2M < "$TESTS_SRCDIR/makeL111"
mkcrl L112 0D 3M < "$TESTS_SRCDIR/makeL112"
mkcrl L21 0D 3M < "$TESTS_SRCDIR/makeL21"

t4s_done
