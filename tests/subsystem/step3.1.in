#!@SHELL_BASH@

# set environment variables if not set
THIS_SCRIPT_DIR=$(dirname $0)
. $THIS_SCRIPT_DIR/../../etc/envir.setup

. $RPKI_ROOT/lib/util/trap_errors

# test functions
. $RPKI_ROOT/tests/test.include

echo "Loading data"
rsync_aur -s -t $RPKI_PORT -f test3.1a.log -w -e -i -d $THIS_SCRIPT_DIR/testcases || \
  check_errs $? "$0 rsync_aur on test3.1a.log failed"
rsync_aur -s -t $RPKI_PORT -f test3.1b.log -w -e -i -d $THIS_SCRIPT_DIR/testcases || \
  check_errs $? "$0 rsync_aur on test3.1b.log failed"

echo $0 PASS
