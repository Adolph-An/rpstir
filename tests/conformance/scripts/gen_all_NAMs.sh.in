#!@SHELL_BASH@

@SETUP_ENVIRONMENT@
@trap_errors@

usage () {
    echo >&2 "Usage: $0 [options]"
    echo >&2
    echo >&2 "Options:"
    echo >&2 "  -P        Apply patches instead of prompting user to edit (default = false)"
    echo >&2 "  -h        Display this help message"
}

X="$TESTS_BUILDDIR/make_test_name.sh"

while getopts Ph opt
do
    case $opt in
        P)
            X="$X -P"
            ;;

        h)
            usage
            exit 0
            ;;
    esac
done
shift $((OPTIND - 1))
if [ $# != "0" ]
then
    usage
    exit 1
fi


